start: email_lines

email_lines := (email | ~fb )*

estr     := /[a-z0-9!#$%&'*+/=?^_`{|}~-]/
beforeAt := estr+ (/[.]/ estr+)*
az09     := /[a-z0-9]/
az09dash := /[a-z0-9-]*[a-z0-9]/
part     := az09 az09dash? /[.]/
afterAt  := part+ az09 az09dash?
email    := " " beforeAt ~/@/ " " afterAt ~/[\n]/ "\n"
fb       := /[^\n]*[\n]/
